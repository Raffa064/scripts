# alias uplogs="cat /.update-logs"
alias bashrc="nano /etc/bash.bashrc && source /etc/bash.bashrc"
alias vsc="cat ~/.config/code-server/config.yaml && code-server"
alias check="checkpoint"

for i in $(ls /scripts/.js); do
 if [ ! -d "/scripts/.js/$i" ]; then
  alias $i="source /scripts/.js/runscript -js $i"
  alias ".$i"="script $i -js"
 fi
done

for i in $(ls /scripts); do
 if [ ! -d "/scripts/$i" ]; then
  alias $i="source /scripts/runscript $i"
  alias ".$i"="script $i"
 fi
done

load-checkpoints
desc setup

backup

echo updatading......
apt-get upgrade -y && apt-get update -y

clear
